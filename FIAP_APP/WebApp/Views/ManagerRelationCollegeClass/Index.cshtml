@model IEnumerable<WebApp.Models.Dto.RelationsClassStudent.RelationsClassStudentDto>

@{
    ViewData["Title"] = "Lista Relações Alunos e Cursos";
}

<h1>@ViewData["Title"]</h1>

<partial name="Error" />

<p>
    <a class="btn btn-info" asp-action="Create">Nova Relação</a>
</p>
<partial name="_ListRelations" />



<div id="myModal" class="modal fade in">
    <div class="modal-dialog">
        <div class="modal-content">
            <div id="myModalContent"></div>
        </div>
    </div>
</div>


@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }

         <script>
                $(document).ready(function () {
                    SetModal();
                });
            </script>
}
